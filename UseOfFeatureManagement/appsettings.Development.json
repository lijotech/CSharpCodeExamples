{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FeatureManagement": {
    "BooleanFeature": false,
    "PercentageFeature": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 30
          }
        }
      ]
    },
    "CustomLanguageFilter": {
      "EnabledFor": [
        {
          "Name": "LanguageFilter",
          "Parameters": {
            "AllowedLanguages": [
              "en-GB",
              "en-US"
            ]
          }
        }
      ]
    }
  }
}
