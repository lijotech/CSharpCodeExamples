{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FeatureManagement": {
    "BooleanFeature": false,
    "PercentageFeature": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 30
          }
        }
      ]
    },
    "CustomLanguageFeature": {
      "EnabledFor": [
        {
          "Name": "LanguageFilter",
          "Parameters": {
            "AllowedLanguages": [
              "en-GB",
              "en-US"
            ]
          }
        }
      ]
    },
    "TimeWindowFeature": {
      "EnabledFor": [
        {
          "Name": "TimeWindow",
          "Parameters": {
            "Start": "2024-05-01T10:00:00+00:00",
            "End": "2024-05-30T11:00:00+00:00"
          }
        }
      ]
    },
    "TargetingFeature": {
      "EnabledFor": [
        {
          "Name": "Targeting",
          "Parameters": {
            "Audience": {
              "Users": [ "User1", "User2" ],
              "Groups": [
                {
                  "Name": "BetaTesters",
                  "RolloutPercentage": 50
                }
              ],
              "DefaultRolloutPercentage": 0
            }
          }
        }
      ]
    }
  }
}
